{
  "name": "@harper-concierge/seon-sdk",
  "version": "1.5.1",
  "description": "This module provides a TypeScript class, Seon, to interact with Seon's Fraud API. The Seon class allows you to send fraud detection requests using the provided API key and endpoint URL.",
  "license": "MIT",
  "author": {
    "name": "Harper Concierge",
    "email": "developers@harperconcierge.com",
    "url": "https://www.harperconcierge.com"
  },
  "contributors": [
    {
      "name": "Harper Concierge",
      "email": "developers@harperconcierge.com",
      "url": "https://www.harperconcierge.com"
    },
    {
      "name": "Dmitrii Selikhov",
      "email": "dmitrii.selikhov@gmail.com",
      "url": "https://www.linkedin.com/in/dimetrix"
    }
  ],
  "email": "developers@harperconcierge.com",
  "scripts": {
    "build": "tsup ./src",
    "watch": "tsup ./src --watch",
    "run": "node dist/index.js",
    "dev": "npm run watch & nodemon dist/index.js",
    "lint": "eslint src/**/*.ts --fix",
    "prettier": "prettier --write src/** package.json README.md",
    "format": "npm run lint && npm run prettier && npm run type",
    "bootstrap": "npm install && npm format",
    "type": "tsc --noEmit --incremental",
    "tsc:clean": "tsc --build --clean",
    "prepare": "husky && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs": "typedoc --entryPoints ./src/index.ts --out docs --plugin typedoc-plugin-markdown",
    "prepublishOnly": "npm run clean && npm run format && npm run test && npm run build && npm run docs",
    "clean": "rm -rf dist docs && npm run tsc:clean"
  },
  "homepage": "https://github.com/harper-concierge/seon-sdk#readme",
  "url": "https://github.com/harper-concierge/seon-sdk/issue",
  "bugs": {
    "url": "https://github.com/harper-concierge/seon-sdk/issues"
  },
  "keywords": [
    "seon",
    "fraud",
    "fraud-detection",
    "fraud-prevention",
    "anti-fraud",
    "aml",
    "anti-money-laundering",
    "device-fingerprinting",
    "risk-assessment",
    "security",
    "fintech",
    "ecommerce",
    "gaming",
    "igaming",
    "typescript",
    "api",
    "sdk",
    "email-validation",
    "phone-validation",
    "ip-intelligence",
    "machine-learning",
    "real-time",
    "compliance"
  ],
  "files": [
    "dist",
    "src",
    "docs",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "dependencies": {
    "react": "19.1.1",
    "typedoc": "0.28.13",
    "typedoc-plugin-markdown": "4.9.0"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.5.2",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "eslint": "9.36.0",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "lint-staged": "16.2.0",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.ts": [
      "tsc --noEmit"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
