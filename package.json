{
  "name": "seon-sdk",
  "version": "1.1.0",
  "description": "This module provides a TypeScript class, Seon, to interact with Seon's Fraud API. The Seon class allows you to send fraud detection requests using the provided API key and endpoint URL.",
  "license": "MIT",
  "author": {
    "name": "Dmitrii Selikhov",
    "email": "dmitrii.selikhov@gmail.com",
    "url": "https://www.linkedin.com/in/dimetrix"
  },
  "contributors": [
    {
      "name": "Dmitrii Selikhov",
      "email": "dmitrii.selikhov@gmail.com",
      "url": "https://www.linkedin.com/in/dimetrix"
    }
  ],
  "email": "dmitrii.selikhov@gmail.com",
  "scripts": {
    "build": "tsup ./src",
    "watch": "tsup ./src --watch",
    "run": "node dist/index.js",
    "dev": "pnpm run watch & nodemon dist/index.js",
    "lint": "eslint src/**/*.ts --fix",
    "prettier": "prettier --write src/** package.json README.md",
    "format": "pnpm run lint && pnpm run prettier && pnpm type",
    "bootstrap": "pnpm install && pnpm format",
    "type": "tsc --noEmit --incremental",
    "tsc:clean": "tsc --build --clean",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs": "typedoc --entryPoints ./src/index.ts --out docs --plugin typedoc-plugin-markdown"
  },
  "homepage": "https://github.com/idimetrix/seon-sdk#readme",
  "url": "https://github.com/idimetrix/seon-sdk/issue",
  "bugs": {
    "url": "https://github.com/idimetrix/seon-sdk/issues"
  },
  "keywords": [],
  "files": [
    "dist",
    "docs",
    "package.json",
    "README.md"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "dependencies": {
    "react": "19.1.0",
    "typedoc": "0.28.7",
    "typedoc-plugin-markdown": "4.7.0"
  },
  "devDependencies": {
    "@eslint/js": "9.31.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.0.14",
    "@typescript-eslint/eslint-plugin": "8.37.0",
    "@typescript-eslint/parser": "8.37.0",
    "eslint": "9.31.0",
    "husky": "9.1.7",
    "jest": "30.0.4",
    "lint-staged": "16.1.2",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "tsup": "8.5.0",
    "typescript": "5.8.3"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.ts": [
      "tsc --noEmit"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
